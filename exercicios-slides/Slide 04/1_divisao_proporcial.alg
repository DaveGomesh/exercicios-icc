algoritmo "divisao_proporcial"
//Desenvolvido no VisuAlg 2.0
// Seção de Declarações 
var
   ptA , ptB , ptC , x, A, B, C: real
inicio
// Seção de Comandos
   leia(ptA , ptB , ptC)
   
   x <- 780000 / (PtA + PtB + PtC)
   
   A <- ptA * x
   B <- ptB * x
   C <- ptC * x
   
   escreval(A, B, C)
fimalgoritmo
